         <img src="/assets/attentionglove2.png" alt="Second slide">
      </a>
      </div>

      <div class="item">
               <a href="/t/anatomie-style">
         <img src="/assets/anatomie2.jpg" alt="Second slide">
      </a>
      </div>
      <div class="item">
         
         <img src="../assets/join2.png" alt="third slide">
      </a>
      </div>
      <div class="item">
         
         <img src="/assets/cart3.jpeg" alt="fourth slide" >
      </a>
      </div>
   </div>
   <!-- Carousel nav -->
   <a class="carousel-control left" href="#myCarousel" 
      data-slide="prev">&lsaquo;</a>
   <a class="carousel-control right" href="#myCarousel" 
      data-slide="next">&rsaquo;</a>
   <!-- Controls buttons -->
  
</div> 
<script>

<script>
   $(function(){
      // Initializes the carousel
      $(".start-slide").click(function(){
         $("#myCarousel").carousel('cycle');
      });
      // Stops the carousel
      $(".pause-slide").click(function(){
         $("#myCarousel").carousel('pause');
      });
      // Cycles to the previous item
      $(".prev-slide").click(function(){
         $("#myCarousel").carousel('prev');
      });
      // Cycles to the next item
      $(".next-slide").click(function(){
         $("#myCarousel").carousel('next');
      });
      // Cycles the carousel to a particular frame 
      $(".slide-one").click(function(){
         $("#myCarousel").carousel(0);
      });
      $(".slide-two").click(function(){
         $("#myCarousel").carousel(1);
      });
      $(".slide-three").click(function(){
         $("#myCarousel").carousel(2);
      });
   });
</script>

