         class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
   </ol>   
   <!-- Carousel items -->
   <div class="carousel-inner">
      <div class="item active">
         <a href="/t/mulco-watches">
         <img src="../assets/mulco3.jpg" alt="First slide">
      </a>
      </div>
      <div class="item">
                  <a href="/t/patrick-gibbons-handmade">
         <img src="/assets/patrickgibbons.jpg" alt="Second slide">
      </a>
      </div>
