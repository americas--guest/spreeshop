


<!--  Carousel - consult the Twitter Bootstrap docs at 
      http://twitter.github.com/bootstrap/javascript.html#carousel -->

      
<div id="myCarousel" class="carousel slide" data-ride="carousel">
 
   <!-- Carousel indicators -->
   <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" 
         class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
   </ol>   
   <!-- Carousel items -->
   <div class="carousel-inner">
      <div class="item active">
         <img src="../assets/swinging3.jpeg" alt="First slide">
         <div class="carousel-caption">
          <h2>At 19th Hole swag we promote the golf lifestyle: focus, fashion, and birdies!</h2></div>
      </div>
      <div class="item">
         <img src="/assets/cart3.jpeg" alt="Second slide" class="sepia">
        <div class="carousel-caption">
          <h1>Flash sales for brands targeting the golfer lifestyle!</h1></div>
      </div>
      <div class="item">
         <img src="/assets/glove3.jpeg" alt="Second slide">
        <div class="carousel-caption">
          <h1>Sign up now for insider access to top designer brands!</h1></div>
      </div>
      <div class="item">
         <img src="assets/stogies3.jpeg" alt="Third slide" class="sepia">
         <div class="carousel-caption">
          <h1>19th Hole Swag has what you need to look like a pro!</h1></div>
      </div>
   </div>
   <!-- Carousel nav -->
   <a class="carousel-control left" href="#myCarousel" 
      data-slide="prev">&lsaquo;</a>
   <a class="carousel-control right" href="#myCarousel" 
      data-slide="next">&rsaquo;</a>
   <!-- Controls buttons -->
  
</div> 
<script>

<script>
   $(function(){
      // Initializes the carousel
      $(".start-slide").click(function(){
         $("#myCarousel").carousel('cycle');
      });
      // Stops the carousel
      $(".pause-slide").click(function(){
         $("#myCarousel").carousel('pause');
      });
      // Cycles to the previous item
      $(".prev-slide").click(function(){
         $("#myCarousel").carousel('prev');
      });
      // Cycles to the next item
      $(".next-slide").click(function(){
         $("#myCarousel").carousel('next');
      });
      // Cycles the carousel to a particular frame 
      $(".slide-one").click(function(){
         $("#myCarousel").carousel(0);
      });
      $(".slide-two").click(function(){
         $("#myCarousel").carousel(1);
      });
      $(".slide-three").click(function(){
         $("#myCarousel").carousel(2);
      });
   });
</script>

